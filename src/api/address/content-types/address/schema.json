{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address1": {
      "type": "string",
      "required": true
    },
    "address2": {
      "type": "string"
    },
    "address3": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "mobileNo": {
      "type": "biginteger",
      "required": true,
      "max": "9999999999",
      "min": "1000000000"
    },
    "pinCode": {
      "type": "biginteger",
      "required": true,
      "max": "999999",
      "min": "100000"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "addresses"
    },
    "district": {
      "type": "string",
      "required": true
    }
  }
}
