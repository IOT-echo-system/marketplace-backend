{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "paymentInfo": {
      "type": "json"
    },
    "txnId": {
      "type": "string"
    },
    "amount": {
      "type": "integer",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "CREATED",
        "PLACED",
        "DELIVERED",
        "CANCELLED"
      ],
      "default": "CREATED",
      "required": true
    },
    "shippingAddress": {
      "type": "json",
      "required": true
    },
    "billingAddress": {
      "type": "json",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "private": true,
      "inversedBy": "orders"
    },
    "products": {
      "displayName": "Product",
      "type": "component",
      "repeatable": true,
      "component": "product.product",
      "required": true
    }
  }
}
