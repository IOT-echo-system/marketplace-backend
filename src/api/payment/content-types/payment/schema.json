{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order"
    },
    "amount": {
      "type": "decimal",
      "required": true
    },
    "discount": {
      "type": "decimal",
      "required": true
    },
    "gst": {
      "type": "decimal"
    },
    "grandTotal": {
      "type": "decimal",
      "required": true
    },
    "verify": {
      "type": "json"
    },
    "finalState": {
      "type": "json"
    },
    "paymentOrder": {
      "type": "json",
      "required": true
    },
    "paymentId": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "CREATED",
        "ATTEMPT",
        "FAILURE",
        "SUCCESS"
      ],
      "required": true
    },
    "orderId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "mode": {
      "type": "enumeration",
      "enum": [
        "CASH",
        "COD",
        "RAZORPAY"
      ],
      "default": "RAZORPAY",
      "required": true
    },
    "collectedBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "payment"
    }
  }
}
