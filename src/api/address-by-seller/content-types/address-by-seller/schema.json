{
  "kind": "collectionType",
  "collectionName": "address_by_sellers",
  "info": {
    "singularName": "address-by-seller",
    "pluralName": "address-by-sellers",
    "displayName": "Address by seller"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "address1": {
      "type": "string",
      "required": true,
      "minLength": 1,
      "trim": true
    },
    "address2": {
      "type": "string"
    },
    "address3": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "state": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "district": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "pinCode": {
      "type": "integer",
      "required": true
    },
    "mobileNo": {
      "type": "biginteger",
      "required": true,
      "max": "9999999999",
      "min": "1000000000"
    }
  }
}
